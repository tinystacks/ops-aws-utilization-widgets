    "lodash.isempty": "4.x",
    "lodash.isnil": "4.x",
    "lodash.sortby": "4.x",
    "react": "18.x",
    "react-chartjs-2": "5.x",
    "react-dom": "18.x",
    "react-icons": "4.x",
    "simple-statistics": "7.x"
  }
}
