    "@aws-sdk/client-cloudformation": "3.x",
    "@aws-sdk/client-cloudwatch": "3.x",
    "@aws-sdk/client-cloudwatch-logs": "3.x",
    "@aws-sdk/client-cost-explorer": "3.x",
    "@aws-sdk/client-ec2": "3.x",
    "@aws-sdk/client-ecs": "3.x",
    "@aws-sdk/client-elastic-load-balancing-v2": "3.x",
    "@aws-sdk/client-pricing": "3.x",
    "@aws-sdk/client-rds": "3.x",
    "@aws-sdk/client-s3": "3.x",
    "@aws-sdk/client-sts": "3.x",
    "@chakra-ui/icons": "2.x",
    "@chakra-ui/react": "2.x",
    "@tinystacks/ops-aws-core-widgets": "file:../ops/ops-aws-core-widgets/tinystacks-ops-aws-core-widgets-0.0.27.tgz",
    "@tinystacks/ops-core": "file:../ops/ops-core/tinystacks-ops-core-0.4.0.tgz",
    "@tinystacks/ops-core-widgets": "file:../ops/ops-core-widgets/tinystacks-ops-core-widgets-0.0.16.tgz",
    "@tinystacks/ops-model": "0.x",
